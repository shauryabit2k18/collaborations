{
    "manifest_version":2,
    "name":"Destello",
    "version":"1.0",
    "description": "Extension of Destello App",
    "icons": {
        "128": "logo.png"
     },


    "background": {
        "scripts":["background.js"]
    },

    "browser_action":{
        "default_icon":"logo.png",
        "default_popup":"index.html"
    },

    "permissions": [
        "http://localhost/*",
        "tabs",
        "storage",
        "history"
    ]



}